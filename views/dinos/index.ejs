<h3>It's Dino Time!</h3>
<h4>Who's coming to the Dino Party?</h4>
<form method="GET" action="/dinos">
    <label for="nameFilter">Filter by Name</label>
    <input id="nameFilter" type="text" name="nameFilter">
    <input type="submit">
</form>
<ul>
    <% dinos.forEach(dino => { %>
    <li>
        <p><strong><%= dino.name %></strong> is a
        <%= dino.type %></p>    
    </li>
    <% }) %>
</ul>